{
    "name": "photosphere-backend",
    "version": "1.0.0",
    "description": "",
    "main": "build/index.js",
    "scripts": {
        "start": "node build/index.js",
        "start:dev": "concurrently \"npm run start-db\" \"npm run start-with-dev-db\"",
        "start-db": "insta-mongo --db-port=7001 --rest-port=7000",
        "start-with-dev-db": "cross-env PORT=3000 DB_CONNECTION_STRING=mongodb://localhost:7001 ts-node-dev --respawn src/index.ts",
        "compile": "tsc",
        "compile:watch": "tsc --watch",
        "clean": "rm -rf build"
    },
    "keywords": [],
    "author": "ashley@codecapers.com.au",
    "license": "MIT",
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.16",
        "@types/node": "^18.11.18",
        "concurrently": "^7.2.2",
        "cross-env": "^7.0.3",
        "insta-mongo": "^0.0.6",
        "nodemon": "^2.0.16",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^5.0.0-beta.1",
        "mongodb": "^4.7.0"
    }
}
